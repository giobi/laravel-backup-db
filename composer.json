{
    "name": "giobi/laravel-backup-db",
    "description": "A Laravel package that extends Spatie's backup functionality with a web UI for database backup management",
    "keywords": ["laravel", "backup", "database", "restore", "spatie", "mysql"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Giovanni Battista",
            "email": "giobi@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/contracts": "^10.0|^11.0",
        "spatie/laravel-backup": "^8.0",
        "spatie/db-dumper": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Giobi\\LaravelBackupDb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Giobi\\LaravelBackupDb\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Giobi\\LaravelBackupDb\\LaravelBackupDbServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}